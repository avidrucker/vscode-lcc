{
  "name": "lcc-hover",
  "displayName": "LCC-Hover",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [{
      "command": "lcc-hover.helloWorld",
      "title": "Hello World"
    }],
    "languages": [{
      "id": "lcc",
      "aliases": ["LCC", "lcc"],
      "extensions": [".bin"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "lcc",
      "scopeName": "source.lcc",
      "path": "./syntaxes/lcc.tmLanguage.json"
    }]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.86.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.9"
  }
}
